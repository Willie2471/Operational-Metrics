            costAnalysisData.forEach(job => {
                const billable = parseFloat(getCostAnalysisValue(job, 'Billable Hours')) || 0;
                const budgeted = parseFloat(getCostAnalysisValue(job, 'Budgeted Hours')) || 0;
                const client = getCostAnalysisValue(job, 'Client Name');
                
                totalBillable += billable;
                totalBudgeted += budgeted;
                if (client) uniqueClients.add(client);
            });

            const efficiencyRate = totalBudgeted > 0 ? (totalBillable / totalBudgeted) * 100 : 0;
            const totalWorkdays = totalBillable / STANDARD_WORKDAY_HOURS;
            const avgJobHours = jobCount > 0 ? totalBillable / jobCount : 0;

            const html = `
                <div class="stat-box">
