            const avgBillable = jobCount > 0 ? totalBillable / jobCount : 0;
            const avgBudgeted = jobCount > 0 ? totalBudgeted / jobCount : 0;
            const utilizationRate = budgetedWorkdays > 0 ? (totalWorkdays / budgetedWorkdays) * 100 : 0;
            
            const totalAvailableHours = budgetedWorkdays * STANDARD_WORKDAY_HOURS;
            const windshieldTime = totalAvailableHours - totalBillable;
            const windshieldPercent = totalAvailableHours > 0 ? (windshieldTime / totalAvailableHours) * 100 : 0;
            
            const html = `
                <div class="metric-card highlight">
                    <div class="metric-header">
                        <span class="metric-title">Jobs Completed</span>
                        <span class="metric-icon">üìã</span>
                    </div>
                    <div class="metric-value">${jobCount}</div>
                    <div class="metric-subtitle">Total jobs in selected period</div>
                    <div class="metric-definition">All completed service jobs tracked in Cost Analysis</div>
                </div>

                <div class="metric-card highlight">
                    <div class="metric-header">
                        <span class="metric-title">Active Clients</span>
                        <span class="metric-icon">üë•</span>
                    </div>
                    <div class="metric-value">${uniqueClients.size}</div>
                    <div class="metric-subtitle">Unique clients serviced</div>
                    <div class="metric-definition">Number of different clients with completed jobs</div>
                </div>

                <div class="metric-card">
                    <button class="info-button" onclick="alert('EFFICIENCY RATE\\n\\nFormula: (Billable Hours √∑ Budgeted Hours) √ó 100\\n\\nDefinition: How actual time compares to estimated time for jobs.\\n\\nTargets:\\n‚Ä¢ 75-100% = Excellent (on/under budget)\\n‚Ä¢ <75% = Over budget (inefficient)\\n‚Ä¢ >100% = Under-estimated\\n\\nBest Practice: Aim for 90-95% to ensure profitability.')">‚ÑπÔ∏è</button>
                    <div class="metric-header">
                        <span class="metric-title">Efficiency Rate</span>
                        <span class="metric-icon">‚ö°</span>
                    </div>
                    <div class="metric-value">${formatPercent(efficiencyRate)}</div>
                    <div class="metric-subtitle">Billable vs Budgeted Performance</div>
                    <div class="metric-definition">Actual vs estimated job completion time</div>
                    <div class="metric-target">Target: 75-100% (90-95% optimal)</div>
                    <div class="metric-badge ${efficiencyRate >= 75 && efficiencyRate <= 100 ? 'badge-excellent' : efficiencyRate < 75 ? 'badge-warning' : 'badge-good'}">
                        ${efficiencyRate >= 75 && efficiencyRate <= 100 ? 'üíé Excellent' : efficiencyRate < 75 ? 'Over Budget' : 'Under-Estimated'}
                    </div>
                </div>

                <div class="metric-card">
                    <div class="metric-header">
                        <span class="metric-title">Total Billable Hours</span>
                        <span class="metric-icon">‚è±Ô∏è</span>
                    </div>
                    <div class="metric-value">${totalBillable.toFixed(0)}h</div>
                    <div class="metric-subtitle">${totalWorkdays.toFixed(1)} workdays (8h/day)</div>
                    <div class="metric-definition">Total productive hours across ${jobCount} jobs</div>
                </div>

                <div class="metric-card">
                    <div class="metric-header">
                        <span class="metric-title">Total Budgeted Hours</span>
                        <span class="metric-icon">üìä</span>
                    </div>
                    <div class="metric-value">${totalBudgeted.toFixed(0)}h</div>
                    <div class="metric-subtitle">${budgetedWorkdays.toFixed(1)} budgeted workdays</div>
                    <div class="metric-definition">Total estimated hours for all jobs</div>
                </div>

                <div class="metric-card">
                    <div class="metric-header">
                        <span class="metric-title">Avg Hours Per Job</span>
                        <span class="metric-icon">üìà</span>
                    </div>
                    <div class="metric-value">${avgBillable.toFixed(1)}h</div>
                    <div class="metric-subtitle">Budgeted: ${avgBudgeted.toFixed(1)}h per job</div>
                    <div class="metric-definition">Average time spent per job completion</div>
                </div>

                <div class="metric-card">
                    <button class="info-button" onclick="alert('UTILIZATION RATE\\n\\nFormula: (Actual Workdays √∑ Budgeted Workdays) √ó 100\\n\\nDefinition: Percentage of available time being used productively.\\n\\nTargets:\\n‚Ä¢ 85%+ = Excellent\\n‚Ä¢ 70-84% = Good\\n‚Ä¢ <70% = Low (underutilized)\\n\\nBest Practice: 80-90% is optimal.')">‚ÑπÔ∏è</button>
                    <div class="metric-header">
                        <span class="metric-title">Utilization Rate</span>
                        <span class="metric-icon">üéØ</span>
                    </div>
                    <div class="metric-value">${formatPercent(utilizationRate)}</div>
                    <div class="metric-subtitle">${totalWorkdays.toFixed(1)} / ${budgetedWorkdays.toFixed(1)} days used</div>
                    <div class="metric-definition">Productive capacity being utilized</div>
                    <div class="metric-target">Target: 85%+ (80-90% optimal)</div>
