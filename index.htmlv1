            `;
            
            document.getElementById('efficiencyMetrics').innerHTML = html;
        }

        function displayEfficiencyChart() {
            const monthlyData = {};
            
            costAnalysisData.forEach(job => {
                const dateStr = getCostAnalysisValue(job, 'Job Date');
                if (!dateStr) return;
                
                const parts = dateStr.split('/');
                if (parts.length !== 3) return;
                
                const month = parseInt(parts[0]);
                const billable = parseFloat(getCostAnalysisValue(job, 'Billable Hours')) || 0;
                const budgeted = parseFloat(getCostAnalysisValue(job, 'Budgeted Hours')) || 0;
                
                if (!monthlyData[month]) {
                    monthlyData[month] = { billable: 0, budgeted: 0 };
                }
                monthlyData[month].billable += billable;
                monthlyData[month].budgeted += budgeted;
            });

            const months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'];
            const efficiencyData = months.map((_, i) => {
                const data = monthlyData[i + 1];
                if (!data || data.budgeted === 0) return 0;
                return (data.billable / data.budgeted) * 100;
            });

            const ctx = document.getElementById('efficiencyTrendChart');
            
            if (efficiencyChart) efficiencyChart.destroy();
            
            efficiencyChart = new Chart(ctx, {
                type: 'line',
                data: {
                    labels: months,
                    datasets: [{
                        label: 'Efficiency Rate (%)',
                        data: efficiencyData,
                        borderColor: '#2d5016',
                        backgroundColor: 'rgba(45, 80, 22, 0.1)',
                        borderWidth: 3,
                        tension: 0.4,
                        fill: true,
                        pointRadius: 6,
                        pointHoverRadius: 8,
                        pointBackgroundColor: '#2d5016'
                    },
                    {
                        label: 'Target Range (90-95%)',
                        data: Array(12).fill(92.5),
                        borderColor: '#27ae60',
                        borderWidth: 2,
                        borderDash: [5, 5],
                        pointRadius: 0,
                        fill: false
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: { 
                            display: true, 
                            position: 'top',
                            labels: {
                                font: { size: 13, weight: 'bold' }
                            }
                        },
                        tooltip: {
                            callbacks: {
                                label: ctx => `Efficiency: ${ctx.parsed.y.toFixed(1)}%`
                            }
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: true,
                            max: 120,
                            ticks: {
                                callback: value => value + '%'
                            }
                        }
                    }
                }
            });
        }

        function displayServicePerformance() {
            const services = {};
            
            costAnalysisData.forEach(job => {
                const service = getCostAnalysisValue(job, 'Service') || 'Unknown';
                const billable = parseFloat(getCostAnalysisValue(job, 'Billable Hours')) || 0;
                const budgeted = parseFloat(getCostAnalysisValue(job, 'Budgeted Hours')) || 0;
                const client = getCostAnalysisValue(job, 'Client Name') || '';
                
                if (!services[service]) {
                    services[service] = { 
                        billable: 0, 
                        budgeted: 0, 
                        jobs: 0,
                        clients: new Set()
                    };
                }
                services[service].billable += billable;
                services[service].budgeted += budgeted;
                services[service].jobs += 1;
                if (client) services[service].clients.add(client);
            });

            const rows = Object.entries(services)
                .sort((a, b) => b[1].jobs - a[1].jobs)
                .map(([name, data]) => {
                    const avgBillable = data.billable / data.jobs;
                    const avgBudgeted = data.budgeted / data.jobs;
                    const efficiency = data.budgeted > 0 ? (data.billable / data.budgeted) * 100 : 0;
                    
                    const efficiencyColor = efficiency >= 75 && efficiency <= 100 ? '#27ae60' : 
                                           efficiency < 75 ? '#e74c3c' : '#f39c12';
                    
                    return `
                        <tr>
                            <td><strong>${name}</strong></td>
                            <td>${data.clients.size} clients</td>
                            <td>${avgBillable.toFixed(1)}h</td>
                            <td>${avgBudgeted.toFixed(1)}h</td>
                            <td style="color:${efficiencyColor};font-weight:bold;">${formatPercent(efficiency)}</td>
                            <td><strong>${data.jobs}</strong></td>
                            <td>${data.billable.toFixed(0)}h</td>
                        </tr>
                    `;
                }).join('');
            
            document.getElementById('serviceTableBody').innerHTML = rows || '<tr><td colspan="7" style="text-align:center;">No service data available</td></tr>';
        }

        function displayClientPerformance() {
            const clients = {};
            
            costAnalysisData.forEach(job => {
                const client = getCostAnalysisValue(job, 'Client Name') || 'Unknown';
                const billable = parseFloat(getCostAnalysisValue(job, 'Billable Hours')) || 0;
                const budgeted = parseFloat(getCostAnalysisValue(job, 'Budgeted Hours')) || 0;
                
                if (!clients[client]) {
                    clients[client] = { 
                        billable: 0, 
                        budgeted: 0, 
                        jobs: 0
                    };
                }
                clients[client].billable += billable;
                clients[client].budgeted += budgeted;
                clients[client].jobs += 1;
            });

            const rows = Object.entries(clients)
                .sort((a, b) => b[1].jobs - a[1].jobs)
                .map(([name, data]) => {
                    const efficiency = data.budgeted > 0 ? (data.billable / data.budgeted) * 100 : 0;
                    const avgJobDuration = data.jobs > 0 ? data.billable / data.jobs : 0;
                    
                    const efficiencyColor = efficiency >= 75 && efficiency <= 100 ? '#27ae60' : 
                                           efficiency < 75 ? '#e74c3c' : '#f39c12';
                    
                    return `
                        <tr>
                            <td><strong>${name}</strong></td>
                            <td>${data.jobs}</td>
                            <td>${data.billable.toFixed(1)}h</td>
                            <td>${data.budgeted.toFixed(1)}h</td>
                            <td style="color:${efficiencyColor};font-weight:bold;">${formatPercent(efficiency)}</td>
                            <td>${avgJobDuration.toFixed(1)}h</td>
                        </tr>
                    `;
                }).join('');
            
            document.getElementById('clientTableBody').innerHTML = rows || '<tr><td colspan="6" style="text-align:center;">No client data available</td></tr>';
        }

        function refreshData() {
            closeAlert();
            loadData();
        }

        console.log('üöÄ WS Lawn Care Operations Dashboard - INITIALIZED');
        console.log('üìä Data Source: Cost Analysis Sheet');
        console.log('‚ö° Focus: Efficiency, Productivity, Service Performance');
        initializePickers();
        loadData();
    </script>
